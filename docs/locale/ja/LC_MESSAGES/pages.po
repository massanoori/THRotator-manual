msgid "the Embodiment of Scarlet Devil (Th06)"
msgstr "東方紅魔郷"

#: ../../pages/install.rst:22
msgid "Perfect Cherry Blossom (Th07)"
msgstr "東方妖々夢"

#: ../../pages/install.rst:23
msgid "Imperishable Night (Th08)"
msgstr "東方永夜抄"

#: ../../pages/install.rst:24
msgid "Phantasmagoria of Flower View (Th09)"
msgstr "東方花映塚"

#: ../../pages/install.rst:25
msgid "Shoot the Bullet (Th095)"
msgstr "東方文花帖"

#: ../../pages/install.rst:27
msgid ""
